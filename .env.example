# Настройки подключения к OpenSearch
OPENSEARCH_HOST=https://opensearch.example.timeweb.cloud
OPENSEARCH_USER=your-user
OPENSEARCH_PASSWORD=your-password
OPENSEARCH_INDEX=knowledge-base
OPENSEARCH_VERIFY_SSL=true

# Настройки TimeWeb AI-Агентов
TIMEWEB_API_BASE=https://api.timeweb.cloud
TIMEWEB_API_TOKEN=your-timeweb-token
TIMEWEB_AGENT_ID=your-agent-id
TIMEWEB_TEMPERATURE=0.2
TIMEWEB_TOP_P=0.9

# Общие настройки сервиса
APP_ENV=development
APP_PORT=8000
